{
  "name": "vscode-azure-functions-remote-web",
  "displayName": "vscode-azure-functions-remote-web",
  "publisher": "ms-toolsai",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mypanel.start"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "webOpener": {
      "scheme": "vscode-remote",
      "import": "dist/webOpener.js"
    },
    "commands": [
      {
        "command": "mypanel.start",
        "title": "start panel"
      }
    ]
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm install && npm run clean && npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package": "vsce package",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "host": "npx serve --cors -l 5000"
  },
  "dependencies": {
    "@azure/arm-app": "^1.0.0-alpha.20220330.1",
    "@azure/identity": "^2.1.0",
    "@azure/identity-vscode": "^1.0.0",
    "@azure/keyvault-keys": "^4.5.0",
    "@msgpack/msgpack": "^2.7.2",
    "@vs/tunnels-connections": "^1.0.7280",
    "@vs/tunnels-contracts": "^1.0.7280",
    "@vs/tunnels-management": "^1.0.7280",
    "@vscode/test-electron": "^2.1.5",
    "adm-zip": "^0.5.9",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "graceful-fs": "^4.2.10",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "node-rsa": "^1.1.1",
    "p-retry": "^5.1.1",
    "process": "^0.11.10",
    "retry": "^0.13.1",
    "safe-buffer": "^5.2.1",
    "stream-browserify": "^3.0.0",
    "tsc": "^2.0.4",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "vscode-uri": "^3.0.3"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.7",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.70.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vscode-internal/remote-web-tunnels": "^1.4.0",
    "@vscode/test-web": "^0.0.29",
    "assert": "^2.0.0",
    "eslint": "^8.22.0",
    "mocha": "^10.0.0",
    "process": "^0.11.10",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
